{
    "version": 2,
    "builds": [
      {
        "src": "client/build/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/auth",
        "dest": "backend/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "client/build/index.html"
      }
    ]
  }
  